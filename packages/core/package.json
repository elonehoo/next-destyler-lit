{
  "name": "@destyler/core",
  "type": "module",
  "version": "0.0.6",
  "description": "a minimal implementation of xstate for UI machines",
  "author": "Elone Hoo <elonehoo@gmail.com>",
  "license": "MIT",
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "default": "./dist/index.mjs"
    },
    "./*": "./*"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "*.d.ts",
    "dist"
  ],
  "scripts": {
    "clear": "rimraf ./dist",
    "prepublishOnly": "pnpm run build",
    "build": "pnpm run clear && unbuild",
    "stub": "unbuild --stub"
  },
  "dependencies": {
    "@destyler/store": "workspace:*",
    "klona": "^2.0.6"
  },
  "devDependencies": {
    "@destyler/utils": "workspace:*"
  }
}
